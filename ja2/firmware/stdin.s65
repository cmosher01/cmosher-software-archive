         .FEATURE LABELS_WITHOUT_COLONS

EOF	=   $FF
STKBAS	=   $0100
READC	=   $C080
LATCHC	=   $C081
KEYIN	=   $FD1B
MONRTS	=   $FF58

STDIN	JSR MONRTS
	TSX
	LDA STKBAS,X
	ASL
	ASL
	ASL
	ASL
	TAX

	LDA LATCHC,X
REREAD	LDA READC,X
	BPL REREAD
	CMP #EOF
	BNE EXIT
	JSR KEYIN
EXIT	RTS

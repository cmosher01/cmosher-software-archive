$(function() {

	module("Util");

	test("forEach nominal", function() {
		var r, rr;

		r = ["a","b","c"];

		rr = [];
		Util.prototype.forEach(r,function(v) {
			rr.push(v);
		});

		deepEqual(rr,r);
	});

	test("forEach undefined", function() {
		var r, rr;

		r = ["a",undefined,"c"];

		rr = [];
		Util.prototype.forEach(r,function(v) {
			rr.push(v);
		});

		deepEqual(rr,["a","c"]);
	});

	test("forEach deleted", function() {
		var r, rr;

		r = ["a","b","c"];
		delete r[1];

		rr = [];
		Util.prototype.forEach(r,function(v) {
			rr.push(v);
		});

		deepEqual(rr,["a","c"]);
	});

	test("forEach sparse", function() {
		var r, rr;

		r = [];
		r[0] = "a";
		r[10000] = "b";

		rr = [];
		Util.prototype.forEach(r,function(v) {
			rr.push(v);
		});

		deepEqual(rr,["a","b"]);
	});





	test("consolodate nominal", function() {
		var r, rr;

		r = ["a","b","c"];
		delete r[1];
		deepEqual(r,["a",undefined,"c"]);

		rr = Util.prototype.consolodate(r);
		deepEqual(rr,["a","c"]);
	});
});

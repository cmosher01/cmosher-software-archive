<?xml version="1.0" encoding="UTF-8"?>
<panel columns="1">





    <menubar weightx="1">
        <menu text="File" mnemonic="0">
            <menuitem text="New" mnemonic="0" action="blankNew" />
            <separator />
            <menuitem text="Preferences..." mnemonic="0" action="preferences" />
            <separator />
            <menuitem text="Exit" mnemonic="1" action="exitApplication" />
        </menu>
    </menubar>





    <splitpane divider="220" weighty="1">



        <tree name="treeRequests" line="false" angle="true" action="clickCount">
            <node name="listToday" text="today" />
            <node name="listRecent" text="recent" expanded="false" />
            <node name="listOld" text="old" expanded="false" />
        </tree>



        <splitpane orientation="vertical">
            <panel columns="1" gap="4">
                <panel columns="3">
                    <panel columns="2" gap="4" weightx="4" valign="bottom">
                        <label text="client:" />
                        <textfield name="clientName" columns="30" action="setDirty" weightx="1" />
                        <label text="topic:" />
                        <textfield name="topic" columns="30" action="setDirty" weightx="1" />
                    </panel>
                    <label weightx="1" />
                    <panel columns="1" gap="2">
                        <button text="  New    " icon="/icon/copy.gif" action="blankNew" />
                        <button text="Copy to New" icon="/icon/copy.gif" action="copyToNew" />
                    </panel>
                </panel>

                <panel columns="3">
                    <panel gap="3">
                        <label text="age: " />
                        <label name="minAgeLabel" text="min" />
                        <textfield name="minAge" columns="3" alignment="right" text="" action="setDirty" />
                        <label text="-" />
                        <textfield name="maxAge" columns="3" alignment="right" action="setDirty" />
                        <label name="maxAgeLabel" text="max (years incl.)" />
                    </panel>
                    <label weightx="1" />
                    <panel gap="3">
                        <label name="createdByLabel" text="created by: " />
                        <label name="createdBy" />
                    </panel>
                </panel>

                <panel gap="3">
                    <label text="kids: " />
                    <label name="minAgeKidLabel" text="min" />
                    <textfield name="minAgeKid" columns="3" alignment="right" action="setDirty" />
                    <label text="-" />
                    <textfield name="maxAgeKid" columns="3" alignment="right" action="setDirty" />
                    <label name="maxAgeKidLabel" text="max (years incl.)" />

                    <togglebutton name="bothSexesKid" text="Both" group="groupGender" action="setDirty" selected="true" />
                    <togglebutton name="maleKid" text="Male only" group="groupGender" action="setDirty" />
                    <togglebutton name="femaleKid" text="Female only" group="groupGender" action="setDirty" />

                    <label text="  " />
                    <checkbox name="noKids" text="without" action="setDirty" />
                </panel>

                <panel gap="1">
                    <label text="inc:   " />
                    <label name="incLabel0" text="0" />
                    <checkbox name="inc0" text="20" action="setDirty" />
                    <checkbox name="inc20" text="30" action="setDirty" />
                    <checkbox name="inc30" text="40" action="setDirty" />
                    <checkbox name="inc40" text="50" action="setDirty" />
                    <checkbox name="inc50" text="60" action="setDirty" />
                    <checkbox name="inc60" text="75" action="setDirty" />
                    <checkbox name="inc75" text="100" action="setDirty" />
                    <checkbox name="inc100" text="150+" action="setDirty" />
                    <checkbox name="inc150" action="setDirty" />
                    <label name="incLabelK" text="  ($K)     " />
                    <checkbox name="incPNA" text="PNA" action="setDirty" />
                </panel>

                <panel gap="6">
                    <label text="eth: " />
                    <checkbox name="ethBlack" text="Black" action="setDirty" />
                    <checkbox name="ethHispanic" text="Hispanic" action="setDirty" />
                    <checkbox name="ethWhite" text="White" action="setDirty" />
                    <checkbox name="ethAsian" text="Asian" action="setDirty" />
                    <checkbox name="ethPacific" text="Pacific" action="setDirty" />
                    <checkbox name="ethIndian" text="Indian" action="setDirty" />
                    <checkbox name="ethOther" text="Other" action="setDirty" />
                </panel>

                <panel gap="4" columns="5">
                    <label text="edu: " />
                    <checkbox name="eduSomeHS" text="some HS" action="setDirty" />
                    <checkbox name="eduHSGrad" text="HS grad" action="setDirty" />
                    <checkbox name="eduSomeCol" text="some College" action="setDirty" />
                    <checkbox name="eduColDeg" text="College degree" action="setDirty" />
                    <label />
                    <checkbox name="eduSomeGrad" text="some grad" action="setDirty" />
                    <checkbox name="eduMasters" text="Masters" action="setDirty" />
                    <checkbox name="eduPro" text="Doctor/Law/Pro" action="setDirty" />
                </panel>

                <panel gap="8">
                    <label text="mar:" />
                    <checkbox name="marSingle" text="Single" action="setDirty" />
                    <checkbox name="marDivorced" text="Widow/Div/Sep" action="setDirty" />
                    <checkbox name="marMarried" text="Married" action="setDirty" />
                    <checkbox name="marPartner" text="Partner" action="setDirty" />
                </panel>

                <panel gap="6">
                    <label text="sex:" />
                    <togglebutton name="bothSexes" text="Both" group="groupGender" action="setDirty" selected="true" />
                    <togglebutton name="male" text="Male only" group="groupGender" action="setDirty" />
                    <togglebutton name="female" text="Female only" group="groupGender" action="setDirty" />

                    <label text="                                        " />
                    <button name="xdem" text="XDem" action="xdem" />
                    <label name="xdemLabel" text="" />
                </panel>

                <panel gap="6">
                    <label text="geo:" />
                    <togglebutton name="geoZIP" text="ZIP" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoFIPS" text="FIPS" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoCounty" text="County name" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoMSA" text="MSA" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoDMA" text="DMA" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoState" text="State name" group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoUSCont" text="Continental U.S." group="groupGeo" action="groupGeo" />
                    <togglebutton name="geoUSFull" text="Full U.S." group="groupGeo" action="groupGeo" selected="true" />
                </panel>

                <panel columns="5" gap="4" weighty="1" weightx="1">
                    <label text="enter geos (or Ctrl-V to paste):" />
                    <label />
                    <label />
                    <label text="geo matches:" />
                    <label text="choose geo:" />

                    <textarea name="geoUser" columns="20" weightx="1" weighty="1" action="setDirty" />
                    <button text="&gt;&gt;" action="calcGeo" weighty="1" />
                    <progressbar name="progressCalcGeo" minimum="0" orientation="vertical" weighty="1" />
                    <list name="geoCalc" action="showMatches" weightx="3" weighty="1" />
                    <list name="geoMatches" action="resolveGeo" weightx="3" weighty="1" />
                </panel>
            </panel>

            <panel columns="1" gap="4">
                <panel gap="3">
                    <label text="break out by:" />
                    <checkbox name="breakGeo" text="Geography " action="setDirty" />
                    <checkbox name="breakSex" text="Male/Female " action="setDirty" />

                    <label weightx="1" />

                    <button name="runCount" text="  Run  " icon="/icon/search.gif" action="runCount" />
                    <button name="abortCount" text=" Abort " icon="/icon/abort.gif" action="abort" />
                </panel>

                <table name="tableCount" weightx="1" weighty="1" />

                <panel gap="3">
                    <button text="Full report" icon="/icon/paste.gif" action="reportToClipboard" />
                    <button text="Tab delim." icon="/icon/paste.gif" action="csvToClipboard" />

                    <button text="Debug Info" action="debugInfo" visible="false" />
                    <label weightx="1" />

                    <button name="delete" text="Delete   " icon="/icon/delete.gif" action="delete" />
                    <button name="cancel" text="Cancel   " icon="/icon/undo.gif" action="cancel" />
                    <button name="save" text="   Save    " icon="/icon/save.gif" action="save" />
                </panel>
            </panel>
        </splitpane>



    </splitpane>





</panel>
